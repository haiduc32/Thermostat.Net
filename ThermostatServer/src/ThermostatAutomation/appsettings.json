{
    "ApplicationInsights": {
        "InstrumentationKey": ""
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    /*This is the settings that must be set individually for each home*/
    "Thermostat": {
        "Channels": [
            {
                /* Channel numers start with 0 */
                "Number": 0,
                "Zones": [ "Living", "Office", "Bedroom" ]
            }
            /*,
            {
                "Number": 1,
                "Zones": [ "Foo", "Boo" ]
            }
                */
        ]
           
    }
}
